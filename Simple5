month_num=int(input("Enter month number"))
if(month_num%2!=0):
    if(month_num<=7):
        print("31 days")
    
    else:
        print("30 days")
    

else:
    if(month_num==2):
        print("28 or 29 days")
    
    elif(month_num<7):
        print("30 days")
    
    else:
        print("31 days")
